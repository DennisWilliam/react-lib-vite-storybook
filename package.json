{
	"name": "hst-ui",
	"version": "0.0.3",
	"type": "module",
	"files": [
		"dist",
		"README.md"
	],
	"main": "./dist/hst-ui.umd.js",
	"module": "./dist/hst-ui.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/hst-ui.es.js",
			"require": "./dist/hst-ui.umd.js"
		}
	},
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"prettier-fix": "npx prettier --write .",
		"format-files": "prettier --write .",
		"format-crlf": "prettier --end-of-line crlf --write .",
		"prepare": "husky install",
		"lint-staged": "lint-staged"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"npm run prettier-fix",
			"npm run lint"
		]
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.0.8",
		"@storybook/addon-interactions": "^7.0.8",
		"@storybook/addon-links": "^7.0.8",
		"@storybook/addons": "^7.0.8",
		"@storybook/blocks": "^7.0.8",
		"@storybook/builder-vite": "^7.0.8",
		"@storybook/react": "^7.0.8",
		"@storybook/react-vite": "^7.0.8",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"@vitejs/plugin-react": "^3.0.0",
		"eslint": "^8.40.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"json": "^11.0.0",
		"lint-staged": "^13.2.2",
		"prettier": "2.8.8",
		"prop-types": "^15.8.1",
		"react-router-dom": "^6.6.1",
		"storybook": "^7.0.8",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-dts": "^1.7.1"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^5.3.6"
	}
}
